digraph {

graph [layout = 'dot',
       outputorder = 'edgesfirst',
       bgcolor = 'white',
       rankdir = 'LR']

node [fontname = 'Helvetica',
      fontsize = '10',
      shape = 'circle',
      fixedsize = 'true',
      width = '0.5',
      style = 'filled',
      fillcolor = 'aliceblue',
      color = 'gray70',
      fontcolor = 'gray50']

edge [fontname = 'Helvetica',
     fontsize = '8',
     len = '1.5',
     color = 'gray80',
     arrowsize = '0.5']

  '1' [label = 'a', shape = 'rectangle', height = '0', fontname = 'Arial', fixedsize = 'false'] 
  '2' [label = 'b', shape = 'rectangle', height = '0', fontname = 'Arial', fixedsize = 'false'] 
  '2'->'1' 
}
